    <!--查询单个-->
    <select id="queryById" resultMap="TestListeningJudgementByPictureMap">
        SELECT
            tljbp.test_listening_judgement_by_picture_id,
            tljbp.test_id,
            tljbp.listening_judgement_by_picture_id,
            t.title , -- Alias added for Test's title
            ljp.question,
            ljp.picture_file_path,
            ljp.audio_file_path,
            ljp.answer,
            ljp.date
        FROM
            Test_Listening_Judgement_By_Picture tljbp
                JOIN Listening_Judgement_By_Picture ljp ON tljbp.listening_judgement_by_picture_id = ljp.listening_judgement_by_picture_id
                JOIN Test t ON tljbp.test_id = t.test_id
        WHERE
                tljbp.test_listening_judgement_by_picture_id = #{testListeningJudgementByPictureId}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="TestListeningJudgementByPictureMap">
        select
          test_listening_judgement_by_picture_id, test_id, listening_judgement_by_picture_id
        from Test_Listening_Judgement_By_Picture
        <where>
            <if test="testListeningJudgementByPictureId != null">
                and test_listening_judgement_by_picture_id = #{testListeningJudgementByPictureId}
            </if>
            <if test="testId != null">
                and test_id = #{testId}
            </if>
            <if test="listeningJudgementByPictureId != null">
                and listening_judgement_by_picture_id = #{listeningJudgementByPictureId}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from Test_Listening_Judgement_By_Picture
        <where>
            <if test="testListeningJudgementByPictureId != null">
                and test_listening_judgement_by_picture_id = #{testListeningJudgementByPictureId}
            </if>
            <if test="testId != null">
                and test_id = #{testId}
            </if>
            <if test="listeningJudgementByPictureId != null">
                and listening_judgement_by_picture_id = #{listeningJudgementByPictureId}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="testListeningJudgementByPictureId" useGeneratedKeys="true">
        insert into Test_Listening_Judgement_By_Picture(test_id, listening_judgement_by_picture_id)
        values (#{testId}, #{listeningJudgementByPictureId})
    </insert>

    <insert id="insertBatch" keyProperty="testListeningJudgementByPictureId" useGeneratedKeys="true">
        insert into Test_Listening_Judgement_By_Picture(test_id, listening_judgement_by_picture_id)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.testId}, #{entity.listeningJudgementByPictureId})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="testListeningJudgementByPictureId" useGeneratedKeys="true">
        insert into Test_Listening_Judgement_By_Picture(test_id, listening_judgement_by_picture_id)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.testId}, #{entity.listeningJudgementByPictureId})
        </foreach>
        on duplicate key update
        test_id = values(test_id),
        listening_judgement_by_picture_id = values(listening_judgement_by_picture_id)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update Test_Listening_Judgement_By_Picture
        <set>
            <if test="testId != null">
                test_id = #{testId},
            </if>
            <if test="listeningJudgementByPictureId != null">
                listening_judgement_by_picture_id = #{listeningJudgementByPictureId},
            </if>
        </set>
        where test_listening_judgement_by_picture_id = #{testListeningJudgementByPictureId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from Test_Listening_Judgement_By_Picture where test_listening_judgement_by_picture_id = #{testListeningJudgementByPictureId}
    </delete>

