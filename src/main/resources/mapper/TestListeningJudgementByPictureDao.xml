        <result property="testId" column="test_id" jdbcType="INTEGER"/>
        <result property="listeningJudgementByPictureId" column="listening_judgement_by_picture_id" jdbcType="INTEGER"/>
        <association property="test" column="test_id" javaType="com.example.demo.entity.Test">
            <id property="testId" column="test_id" />
